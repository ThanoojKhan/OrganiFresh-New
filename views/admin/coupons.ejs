      <div class="container">
        <!-- row -->
        
            <div class="col-12 tm-block-col"><br>
                <div class="d-flex justify-content-center mt-4 mb-4">
                  <h3>Coupon Management</h3>
                </div>
                <div class="tm-bg-primary-dark tm-block tm-block-taller tm-block-scroll">
                  <div class="d-flex justify-content-between align-items-center ">
                    <h2 class="tm-block-title">Coupon List</h2>
                    <a href="/admin/addCoupon" class="btn btn-primary btn-block text-uppercase mb-3" style=" width: 250px; border-radius: 10px;">Add New Coupon</a>
                  </div>

                    <table class="table text-center">
                      <thead>
                        <tr>
                          <th scope="col">Name</th>
                          <th scope="col">Start Date</th>
                          <th scope="col">End Date</th>
                          <th scope="col">Actions</th>
                        </tr>
                      </thead>
                    
                      <tbody>
                        <% if(coupons!=null){ %>
                          <%coupons.forEach(coupon=> {%>
                            <tr scope="row">
                              <td>
                                <%=coupon.couponName%>
                              </td>
                              <td>
                                <%=coupon.startDate.toDateString()%>
                              </td>
                              <td>
                                <%=coupon.endDate.toDateString()%>
                              </td>
                              <td>
                                    <ul class="listq wrapperq item justify-content-center mr-4" >
                                      <a href="/admin/editCoupon?id=<%=coupon._id%>" class="link tw addMore mr-2" title="Edit">
                                          <i class=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                                  class="bi bi-pencil" viewBox="0 0 16 16">
                                                  <path
                                                      d="M12.146.146a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-10 10a.5.5 0 0 1-.168.11l-5 2a.5.5 0 0 1-.65-.65l2-5a.5.5 0 0 1 .11-.168l10-10zM11.207 2.5 13.5 4.793 14.793 3.5 12.5 1.207 11.207 2.5zm1.586 3L10.5 3.207 4 9.707V10h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.293l6.5-6.5zm-9.761 5.175-.106.106-1.528 3.821 3.821-1.528.106-.106A.5.5 0 0 1 5 12.5V12h-.5a.5.5 0 0 1-.5-.5V11h-.5a.5.5 0 0 1-.468-.325z" />
                                              </svg></i>
                                      </a>
                                      
                                      <a href="/admin/deleteCoupon?id=<%=coupon._id%>" class="link tw addMore " title="Delete" onclick="return confirm('Are you sure you want to delete this coupon?');">
                                          <i class=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                                  class="bi bi-trash" viewBox="0 0 16 16">
                                                  <path
                                                      d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                                  <path fill-rule="evenodd"
                                                      d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                              </svg></i>
                                      </a>
                                  </ul>
                            </tr>
                            <% }) %>
                              <% } else { %>
                                <tr>
                                  <td colspan="2">Coupons Not found</td>
                                </tr>
                                <% } %>
                      </tbody>
                    </table>
                </div>
            </div>
        </div>



   
      
      <% if(message){ %>
        <script>
          new Swal({
            title: "Success",
            text: "<%=message%>",
            icon: "success",
            showConfirmButton: false,
            timer: 2000
          });
        </script>
        <% } %>
          <% if(msg){ %>
            <script>
              new Swal({
                title: "Error",
                text: "<%=msg%>",
                icon: "error",
                showConfirmButton: false,
                timer: 2000
              });
            </script>
            <% } %>

