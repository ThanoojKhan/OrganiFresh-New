                <div class="col-12 tm-block-col">
                    <div class="d-flex justify-content-end mt-4 mb-4">
                        <form>
                            <input class="bg-dark text-white" type="text"  name="search">
                            <input type="submit" class="" style="height: 30px; background-color: #f56623; color: white; border-color: #f56623;" value="Search">
                        </form>
                    </div>
                    <div class="tm-bg-primary-dark tm-block tm-block-taller tm-block-scroll">
                        <h2 class="tm-block-title">Products List</h2>
                        <table class="table text-center ">
                            <thead>
                                <tr>
                                    <th scope="col">Name</th>
                                    <th scope="col">Brand</th>
                                    <th scope="col">Price</th>
                                    <th scope="col">Category</th>
                                    <th scope="col">Image</th>
                                    <th scope="col-2">Actions</th>
                                    <th scope="col">Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(product.length>0){
                                    for(let i=0;i<product.length;i++){ %>
                                        <div class="d-flex justify-content-center align-items-center">
                                            <tr>
                                                <td>
                                                    <%=product[i].title%>
                                                </td>
                                                <td>
                                                    <%=product[i].brand%>
                                                </td>
                                                <td>
                                                    <%=product[i].price%>
                                                </td>
                                                <td>
                                                    <%=product[i].category.category%>
                                                </td>
                                                <td>
                                                    <div class=""><img style="width: 100px;" src="/proImage/<%=product[i].image[0]%>"
                                                            alt="Avatar Image" class=""></div>
                                                </td>
                        
                                                <td>
                                                    <ul class="listq wrapperq item mt-1">
                                                        <a href="/admin/editProduct?id=<%=product[i]._id%>" class="link tw addMore mr-2" title="Edit">
                                                            <i class=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                                                    class="bi bi-pencil" viewBox="0 0 16 16">
                                                                    <path
