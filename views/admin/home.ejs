                                          <div class="row">
                                            <div class="col mb-3">
                                              <p class="small text-muted mb-1">Start Date</p>
                                              <p>
                                                <%=orders[i].start_date%>
                                              </p>
                                            </div>
                                            <div class="col mb-3">
                                              <p class="small text-muted mb-1">Order Id</p>
                                              <p>
                                                <%=orders[i].orderId%>
                                              </p>
                                            </div>
                                          </div>


                                          <div class="mx-n5 px-5 py-4" style="background-color: #f2f2f2;">
                                            <div class="row">
                                              <% let price=0 %>
                                                <%orders[i].item.forEach(item=> {%>
                                                  <div class="col-md-8 col-lg-9">
                                                    <p>
                                                      <%=item.product.title%>
                                                    </p>
                                                  </div>
                                                  <div class="col-md-4 col-lg-3">
                                                    <% price +=item.price*item.quantity %>
                                                      <p>₹ <%=item.price*item.quantity%>
                                                  </div>
                                                  <%})%>

                                            </div>
                                            <div class="row">
                                              <div class="col-md-8 col-lg-9">
                                                <p class="fw-bold mb-0">Discount</p>
                                              </div>
                                              <div class="col-md-4 col-lg-3">
                                                <p class="fw-bold mb-0">₹ <%=price - parseInt(orders[i].totalPrice)%>
