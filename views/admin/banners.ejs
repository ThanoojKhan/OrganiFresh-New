      <div class="container">
        <!-- row -->

        <div class="col-12 tm-block-col"><br>
          <div class="d-flex justify-content-center mt-4 mb-4">
            <h3>Home Page Controls</h3>
          </div>
          
          <div class="tm-bg-primary-dark tm-block tm-block-taller tm-block-scroll">
            <h2 class="tm-block-title">List</h2>
            <table class="table text-center ">
              <thead>
                <tr>
                  <th scope="col">Heading</th>
                  <th scope="col">Description</th>
                  <th scope="col-2">Actions</th>
                </tr>
              </thead>
              <tbody>
                <div class="d-flex justify-content-center align-items-center">
                  <% if(banners.length> 0){ %>
                    <% for(let i=0; i<banners.length; i++){ %>
                      <tr>

                        <td>
                          <li><%=banners[i].heading1%></li>
                          <li><%=banners[i].heading2%></li>
                          <li><%=banners[i].heading3%></li>
                        </td>
                        <td>
                          <li><%=banners[i].description1%></li>
                          <li><%=banners[i].description2%></li>
                          <li><%=banners[i].description3%></li>
                        </td>


                        <td>
                          <ul class="listq wrapperq item mt-1">
                            <a href="/admin/deleteBanner?id=<%=banners[i]._id%>" class="link ig addMore mr-2"
                              title="Delete" onclick="return confirm('Are you sure you want to delete this Banner');">
                              <i class=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                  fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                                  <path
                                    d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                  <path fill-rule="evenodd"
                                    d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                </svg></i>
                            </a>
                          </ul>
                        </td>
                      </tr>
                      <% } %>
                        <% } else { %>
                          <tr>
                            <td colspan="3">Nothing to Display</td>
                          </tr>
                          <% } %>
                </div>
              </tbody>
            </table><div class="d-flex justify-content-centre mt-4 mb-4">
              <a href="/admin/addBanner" class="btn btn-dark text-white" >Add Banner</a>
            </div>
          </div>
        </div>
      </div>
