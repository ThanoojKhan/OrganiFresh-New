  <%if(message){%>
    <script>
      new Swal({
        title: "Success",
        text: "<%=message%>",
        icon: "success",
        showConfirmButton: false,
        timer: 2000
      });
    </script>
    <%}%>


      <% if(msg){ %>
        <script>
          new Swal({
            title: "Error",
            text: "<%=msg%>",
            icon: "error",
            showConfirmButton: false,
            timer: 2000
          });
        </script>
        <% } %>


